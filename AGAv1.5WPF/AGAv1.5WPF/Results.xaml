<Window x:Class="AGAv1._5WPF.Results"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AGAv1._5WPF"
        mc:Ignorable="d"
        Title="Արդյունքներ" Height="300" Width="308">
    <Grid>
        <DataGrid AlternatingRowBackground="#FFC4B0B0" Name="dataGrid" HorizontalAlignment="Left" Height="210" Margin="10,10,0,0"  VerticalAlignment="Top" Width="278"/>
        <Button x:Name="Delete_by_ID" Content="Delete by ID" HorizontalAlignment="Left" Margin="10,239,0,0" VerticalAlignment="Top" Width="75" Click=" Delete_by_ID_ClickAsync"/>
        <TextBox x:Name="ID" GotMouseCapture="ID_GotMouseCapture" HorizontalAlignment="Left" KeyUp="ID_KeyDown" Height="20" Margin="107,239,0,0" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top" Width="56" TextAlignment="Center" Cursor="Arrow"/>

        <Button x:Name="DeleteAll" Content="Delete All" HorizontalAlignment="Left" Margin="199,239,0,0" VerticalAlignment="Top" Width="89" Click="DeleteAll_ClickAsync"/>
    </Grid>
</Window>
